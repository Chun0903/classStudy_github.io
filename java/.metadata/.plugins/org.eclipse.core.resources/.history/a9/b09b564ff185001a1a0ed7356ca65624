package com.test.object;

public class Eraser {
	
	private String size;
	private String infor;
	
	public String getSize() {
		return size;
	}
	public void setSize(String size) {
		if(size.equals("Large") 
				||size.equals("Medium")
				||size.equals("Small")){
		this.size = size;
		}
	}
	
	
	public String getInfor() {
		return infor;
	}
	public void setInfor(String infor) {
		infor = this.size;
		this.infor = infor;
	}
	
	
	

}
